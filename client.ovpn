client
dev tun
proto udp
remote xxxxxxxxxxxxxxxx xxxx
remote-random
tls-timeout 3
server-poll-timeout 3
resolv-retry infinite
nobind
persist-key
persist-tun
ca /home/xxxxxxx/.sh/ca.crt
tls-auth /home/xxxxxxx/.sh/ta.key 1
ns-cert-type server
comp-lzo
verb 3
auth-user-pass /home/xxxxxxx/.sh/up
route-method exe
route-delay 2
daemon 
writepid /var/run/openvpn/pid
